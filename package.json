  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
