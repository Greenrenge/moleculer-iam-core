{
  "name": "moleculer-iam",
  "version": "0.4.7",
  "description": "Centralized IAM module for moleculer.\nIncluding a certified OIDC provider and an Identity provider for user profile, credentials, and custom claims management.\nCustom claims could be defined/updated by declarative schema which contains claims validation and migration strategy.",
  "author": "QMIT Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/qmit-pro/moleculer-iam.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "nodemon",
    "build": "rm -rf dist && tsc -p tsconfig.json && cpy '**/*' '!**/*.ts' ../dist/ --cwd=src/ --parents",
    "lint": "tslint -p tsconfig.json",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test": "yarn test-with-examples --testPathIgnorePatterns=src/examples",
    "test-with-examples": "node --trace-warnings node_modules/.bin/jest --coverage --detectOpenHandles --forceExit",
    "app:dev": "yarn app:build-server && rm -rf ./dist-app-renderer && INLINE_RUNTIME_CHUNK=false SKIP_PREFLIGHT_CHECK=true NODE_OPTIONS=--openssl-legacy-provider react-app-rewired start",
    "app:build": "yarn app:build-server && rm -rf ./dist-app-renderer && INLINE_RUNTIME_CHUNK=false SKIP_PREFLIGHT_CHECK=true NODE_OPTIONS=--openssl-legacy-provider react-app-rewired build",
    "app:build-server": "tsc -p tsconfig-app-server.json",
    "app:test": "CI=true SKIP_PREFLIGHT_CHECK=true NODE_OPTIONS=--openssl-legacy-provider react-app-rewired test",
    "app:eject": "react-scripts eject"
  },
  "bin": {
    "generate-jwks": "./bin/generate-jwks.js"
  },
  "nodemonConfig": {
    "watch": false,
    "ext": "ts",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node src/examples/index.ts"
  },
  "keywords": [
    "QMIT",
    "moleculer"
  ],
  "config-overrides-path": "./app-renderer/client.build.js",
  "eslintConfig": {
    "extends": "react-app"
  },
